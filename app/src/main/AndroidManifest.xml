<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lin4">

    <application
        android:name="com.example.lin4.util.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.OpenAppActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.lin4.activity.RegisterActivity"></activity>
        <activity android:name="com.example.lin4.activity.LoginActivity"></activity>
        <activity android:name="com.example.lin4.activity.LINActivity" android:screenOrientation="landscape"></activity>
        <activity android:name="com.example.lin4.activity.SuccessActivity"></activity>
    </application>

</manifest>